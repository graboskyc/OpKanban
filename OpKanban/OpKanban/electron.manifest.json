{
  "executable": "OpKanban",
  "splashscreen": {
    "imageFile": "./wwwroot/Splash.png"
  },
  "name": "OpKanban",
  "author": "graboskyc",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.OpKanban.app",
    "productName": "OpKanban",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}